{
    "name": "mpba/tickets-app",
    "description": "Laravel Tickets, the simple helpdesk tickets system pre-installed in Laravel",
    "keywords": ["laravel-tickets","laravel","helpdesk","ticket","support"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5|^8.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^v3.0.0",
        "guzzlehttp/guzzle": "7.8.0",
        "laravel/framework": "^9.0",
        "laravel/ui": "^3.4",
        "mpba/laravel-archivable": "*",
        "mpba/laravel-modules": "*",
        "mpba/laravel-tickets": "*",
        "predis/predis": "^2.2",
        "spatie/laravel-medialibrary": "^10.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "beyondcode/laravel-query-detector": "^1.7",
        "deployer/deployer": "^7.3",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.26",
        "laravel/tinker": "^2.8",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^6.4",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./Packages/mpba/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
